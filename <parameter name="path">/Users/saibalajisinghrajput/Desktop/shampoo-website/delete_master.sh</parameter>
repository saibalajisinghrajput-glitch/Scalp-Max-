<parameter name="content">#!/bin/bash
set -e

cd /Users/saibalajisinghrajput/Desktop/shampoo-website

echo "ğŸš€ Starting master branch deletion process..."

# Check if gh is installed
if ! command -v gh &> /dev/null; then
    echo "ğŸ“¦ Installing GitHub CLI..."
    brew install gh
fi

# Check if logged in
if ! gh auth status &> /dev/null; then
    echo "ğŸ” Please login to GitHub:"
    gh auth login
fi

# Get repo info
OWNER="saibalajisinghrajput-glitch"
REPO="Scalp-Max-"

echo "ğŸ“‹ Setting main as default branch..."
gh api repos/$OWNER/$REPO -X PATCH -f default_branch=main

echo "ğŸ—‘ï¸ Deleting master branch..."
gh api repos/$OWNER/$REPO/git/refs/heads/master -X DELETE

echo "âœ… Master branch deleted successfully!"
echo ""
echo "ğŸ“ Current branches:"
gh repo view $OWNER/$REPO --json defaultBranchRef,name -q '.defaultBranchRef.name'
</parameter>
